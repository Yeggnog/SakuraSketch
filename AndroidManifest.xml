<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sakurasketch">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:logo="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:minSdkVersion="26">

        <activity android:name=".SketchListActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".SketchEditActivity"
            android:parentActivityName=".SketchListActivity">
        </activity>
        <activity android:name=".ColorActivity">
        </activity>
        <activity android:name=".BrushActivity">
        </activity>
        <activity android:name=".LayerListActivity">
        </activity>
        <activity android:name=".ExportActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>